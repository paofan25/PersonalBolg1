{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport { ref, computed, onMounted } from 'vue';\nimport { useStore } from 'vuex';\nimport { useRouter } from 'vue-router';\nimport { Edit } from '@element-plus/icons-vue';\nimport { ElMessage } from 'element-plus';\nexport default {\n  name: 'UserProfile',\n  components: {\n    Edit\n  },\n  setup() {\n    const store = useStore();\n    const router = useRouter();\n    const currentTab = ref('posts');\n    const isOnline = ref(true);\n    const editProfileDialogVisible = ref(false);\n    const profileFormRef = ref(null);\n    const profileSubmitting = ref(false);\n\n    // Áî®Êà∑Êï∞ÊçÆ\n    const user = computed(() => store.state.auth.user);\n\n    // Áî®Êà∑ÁªüËÆ°\n    const userStats = ref({\n      posts: 0,\n      followers: 0,\n      following: 0\n    });\n\n    // Áî®Êà∑Â§¥ÂÉèÁõ∏ÂÖ≥\n    const avatarColor = computed(() => {\n      // Ê†πÊçÆÁî®Êà∑IDÁîüÊàêÂõ∫ÂÆöÈ¢úËâ≤ÔºåÈÅøÂÖçÂà∑Êñ∞ÂèòÂåñ\n      if (user.value && user.value.id) {\n        const hash = hashCode(user.value.id);\n        const hue = hash % 360;\n        return `hsl(${hue}, 70%, 80%)`;\n      }\n      return `hsl(${Math.random() * 360}, 70%, 80%)`;\n    });\n\n    // Áî®Êà∑ÂêçÈ¶ñÂ≠óÊØç\n    const userInitials = computed(() => {\n      if (user.value && user.value.username) {\n        return user.value.username.slice(0, 2);\n      }\n      return 'Áî®Êà∑';\n    });\n\n    // ÁîüÊàêÈöèÊú∫Êï∞Â≠óÁöÑËæÖÂä©ÂáΩÊï∞\n    const hashCode = str => {\n      let hash = 0;\n      for (let i = 0; i < str.length; i++) {\n        const char = str.charCodeAt(i);\n        hash = (hash << 5) - hash + char;\n        hash = hash & hash; // Convert to 32bit integer\n      }\n      return Math.abs(hash);\n    };\n\n    // Ê†áÁ≠æÈ°µÈÖçÁΩÆ\n    const tabs = [{\n      id: 'posts',\n      label: 'ÊñáÁ´†'\n    }, {\n      id: 'favorites',\n      label: 'Êî∂Ëóè'\n    }, {\n      id: 'activities',\n      label: 'Âä®ÊÄÅ'\n    }];\n\n    // ÊñáÁ´†Êï∞ÊçÆ\n    const posts = ref([]);\n\n    // Êî∂ËóèÊï∞ÊçÆ\n    const favorites = ref([]);\n\n    // Âä®ÊÄÅÊï∞ÊçÆ\n    const activities = ref([]);\n\n    // Ëé∑ÂèñÁî®Êà∑‰ø°ÊÅØ\n    const fetchUserData = async () => {\n      try {\n        if (!user.value) {\n          await store.dispatch('auth/getInfo');\n        }\n\n        // Ê®°ÊãüËé∑ÂèñÁî®Êà∑ÁªüËÆ°Êï∞ÊçÆ\n        userStats.value = {\n          posts: Math.floor(Math.random() * 10),\n          followers: Math.floor(Math.random() * 100),\n          following: Math.floor(Math.random() * 50)\n        };\n\n        // Ê®°ÊãüËé∑ÂèñÊñáÁ´†Êï∞ÊçÆ\n        posts.value = Array(userStats.value.posts).fill(0).map((_, index) => ({\n          id: `post-${index}`,\n          title: `ÊàëÁöÑÊñáÁ´† ${index + 1}`,\n          excerpt: 'ËøôÊòØ‰∏ÄÁØáÁ≤æÂΩ©ÁöÑÊñáÁ´†ÔºåÁÇπÂáªÊü•ÁúãËØ¶ÊÉÖ...',\n          coverImage: index % 2 === 0 ? `https://picsum.photos/id/${index + 10}/300/200` : null,\n          likes: Math.floor(Math.random() * 50),\n          comments: Math.floor(Math.random() * 20),\n          createdAt: new Date(Date.now() - Math.random() * 10000000000)\n        }));\n\n        // Ê®°ÊãüËé∑ÂèñÊî∂ËóèÊï∞ÊçÆ\n        const favCount = Math.floor(Math.random() * 5);\n        favorites.value = Array(favCount).fill(0).map((_, index) => ({\n          id: `fav-${index}`,\n          postId: `post-${index}`,\n          title: `Êî∂ËóèÁöÑÊñáÁ´† ${index + 1}`,\n          description: 'ËøôÊòØÊàëÊî∂ËóèÁöÑ‰∏ÄÁØáÁ≤æÂΩ©ÊñáÁ´†...',\n          createdAt: new Date(Date.now() - Math.random() * 10000000000)\n        }));\n\n        // Ê®°ÊãüËé∑ÂèñÂä®ÊÄÅÊï∞ÊçÆ\n        const actCount = Math.floor(Math.random() * 8);\n        const actionTypes = ['ÂèëË°®‰∫ÜÊñáÁ´†', 'ËØÑËÆ∫‰∫ÜÊñáÁ´†', 'ÁÇπËµû‰∫ÜÊñáÁ´†', 'ÂÖ≥Ê≥®‰∫ÜÁî®Êà∑'];\n        const icons = ['‚úçÔ∏è', 'üí¨', 'üëç', 'üîî'];\n        activities.value = Array(actCount).fill(0).map((_, index) => {\n          const typeIndex = Math.floor(Math.random() * actionTypes.length);\n          return {\n            id: `act-${index}`,\n            icon: icons[typeIndex],\n            description: `${actionTypes[typeIndex]}„ÄäÁ≤æÂΩ©ÂÜÖÂÆπ ${index + 1}„Äã`,\n            timestamp: new Date(Date.now() - Math.random() * 10000000000)\n          };\n        });\n      } catch (error) {\n        console.error('Ëé∑ÂèñÁî®Êà∑Êï∞ÊçÆÂ§±Ë¥•:', error);\n        ElMessage.error('Ëé∑ÂèñÁî®Êà∑Êï∞ÊçÆÂ§±Ë¥•ÔºåËØ∑Âà∑Êñ∞ÈáçËØï');\n      }\n    };\n\n    // ÁºñËæëËµÑÊñôË°®Âçï\n    const profileForm = ref({\n      username: '',\n      bio: '',\n      email: ''\n    });\n\n    // Ë°®ÂçïÈ™åËØÅËßÑÂàô\n    const profileRules = {\n      username: [{\n        required: true,\n        message: 'ËØ∑ËæìÂÖ•Áî®Êà∑Âêç',\n        trigger: 'blur'\n      }, {\n        min: 2,\n        max: 20,\n        message: 'ÈïøÂ∫¶Âú® 2 Âà∞ 20 ‰∏™Â≠óÁ¨¶',\n        trigger: 'blur'\n      }],\n      bio: [{\n        max: 200,\n        message: 'ÁÆÄ‰ªã‰∏çËÉΩË∂ÖËøá200‰∏™Â≠óÁ¨¶',\n        trigger: 'blur'\n      }]\n    };\n\n    // ÊâìÂºÄÁºñËæëËµÑÊñôÂØπËØùÊ°Ü\n    const openEditProfileDialog = () => {\n      if (user.value) {\n        profileForm.value = {\n          username: user.value.username || '',\n          bio: user.value.bio || '',\n          email: user.value.email || ''\n        };\n      }\n      editProfileDialogVisible.value = true;\n    };\n\n    // Êèê‰∫§‰∏™‰∫∫ËµÑÊñôË°®Âçï\n    const submitProfileForm = async () => {\n      if (!profileFormRef.value) return;\n      await profileFormRef.value.validate(async valid => {\n        if (valid) {\n          try {\n            profileSubmitting.value = true;\n\n            // Ë∞ÉÁî®APIÊõ¥Êñ∞‰∏™‰∫∫ËµÑÊñô\n            await store.dispatch('auth/updateProfile', {\n              username: profileForm.value.username,\n              bio: profileForm.value.bio\n            });\n            ElMessage({\n              type: 'success',\n              message: '‰∏™‰∫∫ËµÑÊñôÊõ¥Êñ∞ÊàêÂäü'\n            });\n            editProfileDialogVisible.value = false;\n          } catch (error) {\n            console.error('Êõ¥Êñ∞ËµÑÊñôÂ§±Ë¥•:', error);\n            ElMessage({\n              type: 'error',\n              message: 'Êõ¥Êñ∞ËµÑÊñôÂ§±Ë¥•ÔºåËØ∑ÈáçËØï'\n            });\n          } finally {\n            profileSubmitting.value = false;\n          }\n        }\n      });\n    };\n\n    // Ê†ºÂºèÂåñÊó•Êúü\n    const formatDate = date => {\n      return new Date(date).toLocaleDateString('zh-CN', {\n        year: 'numeric',\n        month: 'long',\n        day: 'numeric'\n      });\n    };\n\n    // Êü•ÁúãÊñáÁ´†ËØ¶ÊÉÖ\n    const viewPost = postId => {\n      // Ë∑≥ËΩ¨Âà∞ÊñáÁ´†ËØ¶ÊÉÖÈ°µ\n      router.push(`/blog/post/${postId}`);\n    };\n\n    // ÂØºËà™Ë∑≥ËΩ¨\n    const navigateTo = path => {\n      router.push(path);\n    };\n\n    // È°µÈù¢Âä†ËΩΩÊó∂Ëé∑ÂèñÊï∞ÊçÆ\n    onMounted(() => {\n      fetchUserData();\n    });\n    return {\n      user,\n      userStats,\n      isOnline,\n      currentTab,\n      tabs,\n      posts,\n      favorites,\n      activities,\n      userInitials,\n      avatarColor,\n      formatDate,\n      viewPost,\n      navigateTo,\n      editProfileDialogVisible,\n      profileForm,\n      profileFormRef,\n      profileRules,\n      profileSubmitting,\n      openEditProfileDialog,\n      submitProfileForm\n    };\n  }\n};","map":{"version":3,"names":["ref","computed","onMounted","useStore","useRouter","Edit","ElMessage","name","components","setup","store","router","currentTab","isOnline","editProfileDialogVisible","profileFormRef","profileSubmitting","user","state","auth","userStats","posts","followers","following","avatarColor","value","id","hash","hashCode","hue","Math","random","userInitials","username","slice","str","i","length","char","charCodeAt","abs","tabs","label","favorites","activities","fetchUserData","dispatch","floor","Array","fill","map","_","index","title","excerpt","coverImage","likes","comments","createdAt","Date","now","favCount","postId","description","actCount","actionTypes","icons","typeIndex","icon","timestamp","error","console","profileForm","bio","email","profileRules","required","message","trigger","min","max","openEditProfileDialog","submitProfileForm","validate","valid","type","formatDate","date","toLocaleDateString","year","month","day","viewPost","push","navigateTo","path"],"sources":["E:\\code\\PersonalBolg1\\frontend\\src\\views\\UserProfile.vue"],"sourcesContent":["<template>\r\n  <div class=\"profile-container\">\r\n    <!-- ËÉåÊôØÂä®Áîª -->\r\n    <div class=\"floating-jellyfish\"></div>\r\n    \r\n    <!-- Áî®Êà∑‰ø°ÊÅØÂç°Áâá -->\r\n    <div class=\"profile-header sweet-card\">\r\n      <div class=\"profile-avatar-wrapper\">\r\n        <div class=\"profile-avatar\" :style=\"{ backgroundColor: avatarColor }\">\r\n          {{ userInitials }}\r\n        </div>\r\n        <div class=\"online-status\" :class=\"{ online: isOnline }\"></div>\r\n      </div>\r\n      \r\n      <div class=\"profile-info\">\r\n        <h1 class=\"username\">{{ user ? user.username : 'Âä†ËΩΩ‰∏≠...' }}</h1>\r\n        <p class=\"bio\">{{ user ? (user.bio || 'Ëøô‰∏™‰∫∫ÂæàÊáíÔºå‰ªÄ‰πàÈÉΩÊ≤°ÂÜô~') : 'Âä†ËΩΩ‰∏≠...' }}</p>\r\n        \r\n        <div class=\"stats\">\r\n          <div class=\"stat-item\">\r\n            <span class=\"stat-value\">{{ userStats.posts }}</span>\r\n            <span class=\"stat-label\">ÊñáÁ´†</span>\r\n          </div>\r\n          <div class=\"stat-item\">\r\n            <span class=\"stat-value\">{{ userStats.followers }}</span>\r\n            <span class=\"stat-label\">Á≤â‰∏ù</span>\r\n          </div>\r\n          <div class=\"stat-item\">\r\n            <span class=\"stat-value\">{{ userStats.following }}</span>\r\n            <span class=\"stat-label\">ÂÖ≥Ê≥®</span>\r\n          </div>\r\n        </div>\r\n        \r\n        <div class=\"user-actions\">\r\n          <el-button type=\"primary\" @click=\"openEditProfileDialog\">\r\n            <el-icon><Edit /></el-icon>\r\n            ÁºñËæëËµÑÊñô\r\n          </el-button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- ÂÜÖÂÆπÊ†áÁ≠æÈ°µ -->\r\n    <div class=\"content-tabs sweet-card\">\r\n      <div class=\"tab-header\">\r\n        <button \r\n          v-for=\"tab in tabs\" \r\n          :key=\"tab.id\"\r\n          class=\"tab-btn\"\r\n          :class=\"{ active: currentTab === tab.id }\"\r\n          @click=\"currentTab = tab.id\"\r\n        >\r\n          {{ tab.label }}\r\n        </button>\r\n      </div>\r\n\r\n      <!-- ÊñáÁ´†ÂàóË°® -->\r\n      <div v-if=\"currentTab === 'posts'\" class=\"posts-grid\">\r\n        <div v-if=\"posts.length === 0\" class=\"empty-state\">\r\n          <div class=\"empty-icon\">üìù</div>\r\n          <h3>ÊöÇÊó†ÊñáÁ´†</h3>\r\n          <p>ÊÇ®ËøòÊ≤°ÊúâÂèëÂ∏É‰ªª‰ΩïÊñáÁ´†</p>\r\n          <el-button type=\"primary\" @click=\"navigateTo('/blog/new')\">\r\n            ÂÜôÁ¨¨‰∏ÄÁØáÊñáÁ´†\r\n          </el-button>\r\n        </div>\r\n        <div v-else v-for=\"post in posts\" \r\n             :key=\"post.id\" \r\n             class=\"post-card sweet-card\"\r\n             @click=\"viewPost(post.id)\">\r\n          <div class=\"post-image\" v-if=\"post.coverImage\">\r\n            <img :src=\"post.coverImage\" :alt=\"post.title\">\r\n          </div>\r\n          <div class=\"post-content\">\r\n            <h3>{{ post.title }}</h3>\r\n            <p>{{ post.excerpt }}</p>\r\n            <div class=\"post-meta\">\r\n              <span>{{ formatDate(post.createdAt) }}</span>\r\n              <div class=\"post-stats\">\r\n                <span>üëç {{ post.likes }}</span>\r\n                <span>üí≠ {{ post.comments }}</span>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Êî∂ËóèÂàóË°® -->\r\n      <div v-else-if=\"currentTab === 'favorites'\" class=\"favorites-grid\">\r\n        <div v-if=\"favorites.length === 0\" class=\"empty-state\">\r\n          <div class=\"empty-icon\">‚ù§Ô∏è</div>\r\n          <h3>ÊöÇÊó†Êî∂Ëóè</h3>\r\n          <p>ÊÇ®ËøòÊ≤°ÊúâÊî∂Ëóè‰ªª‰ΩïÂÜÖÂÆπ</p>\r\n          <el-button type=\"primary\" @click=\"navigateTo('/blog')\">\r\n            ÊµèËßàÊñáÁ´†\r\n          </el-button>\r\n        </div>\r\n        <div v-else v-for=\"favorite in favorites\" \r\n             :key=\"favorite.id\" \r\n             class=\"favorite-card sweet-card\"\r\n             @click=\"viewPost(favorite.postId)\">\r\n          <div class=\"favorite-content\">\r\n            <h3>{{ favorite.title }}</h3>\r\n            <p>{{ favorite.description }}</p>\r\n            <div class=\"favorite-meta\">\r\n              <span>{{ formatDate(favorite.createdAt) }}</span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Âä®ÊÄÅÂàóË°® -->\r\n      <div v-else class=\"activities-list\">\r\n        <div v-if=\"activities.length === 0\" class=\"empty-state\">\r\n          <div class=\"empty-icon\">üîî</div>\r\n          <h3>ÊöÇÊó†Âä®ÊÄÅ</h3>\r\n          <p>ÊÇ®ÁöÑÂä®ÊÄÅÂ∞ÜÂú®ËøôÈáåÊòæÁ§∫</p>\r\n        </div>\r\n        <div v-else v-for=\"activity in activities\" \r\n             :key=\"activity.id\" \r\n             class=\"activity-item sweet-card\">\r\n          <div class=\"activity-icon\">{{ activity.icon }}</div>\r\n          <div class=\"activity-content\">\r\n            <p>{{ activity.description }}</p>\r\n            <span class=\"activity-time\">{{ formatDate(activity.timestamp) }}</span>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    \r\n    <!-- ÁºñËæë‰∏™‰∫∫ËµÑÊñôÂØπËØùÊ°Ü -->\r\n    <el-dialog\r\n      v-model=\"editProfileDialogVisible\"\r\n      title=\"ÁºñËæë‰∏™‰∫∫ËµÑÊñô\"\r\n      width=\"500px\"\r\n    >\r\n      <el-form\r\n        ref=\"profileFormRef\"\r\n        :model=\"profileForm\"\r\n        :rules=\"profileRules\"\r\n        label-position=\"top\"\r\n      >\r\n        <el-form-item label=\"Áî®Êà∑Âêç\" prop=\"username\">\r\n          <el-input v-model=\"profileForm.username\" />\r\n        </el-form-item>\r\n        \r\n        <el-form-item label=\"‰∏™‰∫∫ÁÆÄ‰ªã\" prop=\"bio\">\r\n          <el-input\r\n            v-model=\"profileForm.bio\"\r\n            type=\"textarea\"\r\n            :rows=\"3\"\r\n            placeholder=\"‰ªãÁªç‰∏Ä‰∏ãËá™Â∑±Âêß...\"\r\n          />\r\n        </el-form-item>\r\n        \r\n        <el-form-item label=\"ÁîµÂ≠êÈÇÆÁÆ±\" prop=\"email\">\r\n          <el-input v-model=\"profileForm.email\" disabled />\r\n          <div class=\"form-hint\">ÈÇÆÁÆ±ÊöÇ‰∏çÊîØÊåÅ‰øÆÊîπ</div>\r\n        </el-form-item>\r\n      </el-form>\r\n      \r\n      <template #footer>\r\n        <el-button @click=\"editProfileDialogVisible = false\">ÂèñÊ∂à</el-button>\r\n        <el-button type=\"primary\" @click=\"submitProfileForm\" :loading=\"profileSubmitting\">\r\n          ‰øùÂ≠ò\r\n        </el-button>\r\n      </template>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, computed, onMounted } from 'vue'\r\nimport { useStore } from 'vuex'\r\nimport { useRouter } from 'vue-router'\r\nimport { Edit } from '@element-plus/icons-vue'\r\nimport { ElMessage } from 'element-plus'\r\n\r\nexport default {\r\n  name: 'UserProfile',\r\n  components: {\r\n    Edit\r\n  },\r\n  setup() {\r\n    const store = useStore()\r\n    const router = useRouter()\r\n    const currentTab = ref('posts')\r\n    const isOnline = ref(true)\r\n    const editProfileDialogVisible = ref(false)\r\n    const profileFormRef = ref(null)\r\n    const profileSubmitting = ref(false)\r\n    \r\n    // Áî®Êà∑Êï∞ÊçÆ\r\n    const user = computed(() => store.state.auth.user)\r\n    \r\n    // Áî®Êà∑ÁªüËÆ°\r\n    const userStats = ref({\r\n      posts: 0,\r\n      followers: 0,\r\n      following: 0\r\n    })\r\n    \r\n    // Áî®Êà∑Â§¥ÂÉèÁõ∏ÂÖ≥\r\n    const avatarColor = computed(() => {\r\n      // Ê†πÊçÆÁî®Êà∑IDÁîüÊàêÂõ∫ÂÆöÈ¢úËâ≤ÔºåÈÅøÂÖçÂà∑Êñ∞ÂèòÂåñ\r\n      if (user.value && user.value.id) {\r\n        const hash = hashCode(user.value.id)\r\n        const hue = hash % 360\r\n        return `hsl(${hue}, 70%, 80%)`\r\n      }\r\n      return `hsl(${Math.random() * 360}, 70%, 80%)`\r\n    })\r\n    \r\n    // Áî®Êà∑ÂêçÈ¶ñÂ≠óÊØç\r\n    const userInitials = computed(() => {\r\n      if (user.value && user.value.username) {\r\n        return user.value.username.slice(0, 2)\r\n      }\r\n      return 'Áî®Êà∑'\r\n    })\r\n    \r\n    // ÁîüÊàêÈöèÊú∫Êï∞Â≠óÁöÑËæÖÂä©ÂáΩÊï∞\r\n    const hashCode = (str) => {\r\n      let hash = 0\r\n      for (let i = 0; i < str.length; i++) {\r\n        const char = str.charCodeAt(i)\r\n        hash = ((hash << 5) - hash) + char\r\n        hash = hash & hash // Convert to 32bit integer\r\n      }\r\n      return Math.abs(hash)\r\n    }\r\n    \r\n    // Ê†áÁ≠æÈ°µÈÖçÁΩÆ\r\n    const tabs = [\r\n      { id: 'posts', label: 'ÊñáÁ´†' },\r\n      { id: 'favorites', label: 'Êî∂Ëóè' },\r\n      { id: 'activities', label: 'Âä®ÊÄÅ' }\r\n    ]\r\n\r\n    // ÊñáÁ´†Êï∞ÊçÆ\r\n    const posts = ref([])\r\n    \r\n    // Êî∂ËóèÊï∞ÊçÆ\r\n    const favorites = ref([])\r\n    \r\n    // Âä®ÊÄÅÊï∞ÊçÆ\r\n    const activities = ref([])\r\n    \r\n    // Ëé∑ÂèñÁî®Êà∑‰ø°ÊÅØ\r\n    const fetchUserData = async () => {\r\n      try {\r\n        if (!user.value) {\r\n          await store.dispatch('auth/getInfo')\r\n        }\r\n        \r\n        // Ê®°ÊãüËé∑ÂèñÁî®Êà∑ÁªüËÆ°Êï∞ÊçÆ\r\n        userStats.value = {\r\n          posts: Math.floor(Math.random() * 10),\r\n          followers: Math.floor(Math.random() * 100),\r\n          following: Math.floor(Math.random() * 50)\r\n        }\r\n        \r\n        // Ê®°ÊãüËé∑ÂèñÊñáÁ´†Êï∞ÊçÆ\r\n        posts.value = Array(userStats.value.posts).fill(0).map((_, index) => ({\r\n          id: `post-${index}`,\r\n          title: `ÊàëÁöÑÊñáÁ´† ${index + 1}`,\r\n          excerpt: 'ËøôÊòØ‰∏ÄÁØáÁ≤æÂΩ©ÁöÑÊñáÁ´†ÔºåÁÇπÂáªÊü•ÁúãËØ¶ÊÉÖ...',\r\n          coverImage: index % 2 === 0 ? `https://picsum.photos/id/${index + 10}/300/200