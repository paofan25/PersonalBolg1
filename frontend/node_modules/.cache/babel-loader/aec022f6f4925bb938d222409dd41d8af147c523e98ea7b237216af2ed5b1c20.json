{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.find.js\";\nexport default {\n  name: 'BlogView',\n  data() {\n    return {\n      posts: [],\n      loading: true,\n      activeCategory: null,\n      activeTag: null,\n      categories: [{\n        _id: '1',\n        name: '旅行日记',\n        icon: '🧳'\n      }, {\n        _id: '2',\n        name: '美食探索',\n        icon: '🍰'\n      }, {\n        _id: '3',\n        name: '手工制作',\n        icon: '🧶'\n      }, {\n        _id: '4',\n        name: '生活随笔',\n        icon: '✏️'\n      }],\n      popularTags: ['甜点', '旅行', '手工', '阅读', '音乐', '电影', '宠物', '心情'],\n      tagColors: {\n        '甜点': '#FFB6C1',\n        '旅行': '#87CEEB',\n        '手工': '#DDA0DD',\n        '阅读': '#98FB98',\n        '音乐': '#FFA07A',\n        '电影': '#87CEFA',\n        '宠物': '#F0E68C',\n        '心情': '#E6E6FA'\n      }\n    };\n  },\n  computed: {\n    postsCount() {\n      return this.posts.length;\n    },\n    categoriesCount() {\n      return this.categories.length;\n    }\n  },\n  mounted() {\n    // 模拟获取文章数据\n    setTimeout(() => {\n      this.posts = this.getDummyPosts();\n      this.loading = false;\n    }, 1000);\n  },\n  methods: {\n    goToPost(id) {\n      this.$router.push(`/posts/${id}`);\n    },\n    formatDate(dateString) {\n      const date = new Date(dateString);\n      return date.toLocaleDateString('zh-CN', {\n        year: 'numeric',\n        month: 'long',\n        day: 'numeric'\n      });\n    },\n    filterByCategory(categoryId) {\n      this.activeCategory = categoryId;\n      // 在实际应用中，这里会重新获取筛选后的文章\n    },\n    filterByTag(tag) {\n      this.activeTag = this.activeTag === tag ? null : tag;\n      // 在实际应用中，这里会重新获取筛选后的文章\n    },\n    getDummyPosts() {\n      // 模拟文章数据\n      return [{\n        _id: '1',\n        title: '樱花季的京都之旅',\n        excerpt: '春季的京都被樱花装点得如梦似幻，漫步在古老的街道上，感受千年文化与自然的完美融合...',\n        coverImage: 'https://via.placeholder.com/600x400?text=樱花季的京都之旅',\n        category: '旅行日记',\n        tags: ['旅行', '日本', '樱花'],\n        createdAt: '2023-04-12T08:00:00.000Z'\n      }, {\n        _id: '2',\n        title: '自制云朵棉花糖食谱',\n        excerpt: '用简单的材料在家制作蓬松柔软的云朵棉花糖，为下午茶时光增添一抹甜蜜...',\n        coverImage: 'https://via.placeholder.com/600x400?text=自制云朵棉花糖食谱',\n        category: '美食探索',\n        tags: ['甜点', '食谱', '手工'],\n        createdAt: '2023-05-06T10:30:00.000Z'\n      }, {\n        _id: '3',\n        title: '星空主题钩针小物',\n        excerpt: '分享一款简单的星星图案钩针小包教程，适合初学者尝试的可爱手工项目...',\n        coverImage: 'https://via.placeholder.com/600x400?text=星空主题钩针小物',\n        category: '手工制作',\n        tags: ['手工', '钩针', '教程'],\n        createdAt: '2023-06-18T14:15:00.000Z'\n      }];\n    },\n    getCategoryEmoji(category) {\n      const emojiMap = {\n        '旅行日记': '🧳',\n        '美食探索': '🍰',\n        '手工制作': '🧶',\n        '生活随笔': '✏️'\n      };\n      return emojiMap[category] || '📝';\n    },\n    animateEmoji(postId) {\n      const post = this.posts.find(p => p._id === postId);\n      if (post) {\n        post.isAnimating = true;\n        setTimeout(() => {\n          post.isAnimating = false;\n        }, 1000);\n      }\n    },\n    getTagColor(tag) {\n      return this.tagColors[tag] || '#87CEEB';\n    },\n    getCategoryCount(categoryId) {\n      return this.posts.filter(post => {\n        const category = this.categories.find(c => c._id === categoryId);\n        return category && post.category === category.name;\n      }).length;\n    }\n  }\n};","map":{"version":3,"names":["name","data","posts","loading","activeCategory","activeTag","categories","_id","icon","popularTags","tagColors","computed","postsCount","length","categoriesCount","mounted","setTimeout","getDummyPosts","methods","goToPost","id","$router","push","formatDate","dateString","date","Date","toLocaleDateString","year","month","day","filterByCategory","categoryId","filterByTag","tag","title","excerpt","coverImage","category","tags","createdAt","getCategoryEmoji","emojiMap","animateEmoji","postId","post","find","p","isAnimating","getTagColor","getCategoryCount","filter","c"],"sources":["E:\\code\\PersonalBolg1\\frontend\\src\\views\\BlogView.vue"],"sourcesContent":["<template>\n  <div class=\"blog\">\n    <!-- 云朵导航栏 -->\n    <nav class=\"cloud-nav sweet-card\">\n      <div class=\"cloud-nav-content\">\n        <div class=\"nav-cloud\">\n          <router-link to=\"/\" class=\"nav-item\">\n            <span class=\"cloud-icon\">🏠</span>\n            <span>首页</span>\n          </router-link>\n          <router-link to=\"/blog\" class=\"nav-item active\">\n            <span class=\"cloud-icon\">📚</span>\n            <span>博客</span>\n          </router-link>\n          <router-link to=\"/games\" class=\"nav-item\">\n            <span class=\"cloud-icon\">🎮</span>\n            <span>游戏</span>\n          </router-link>\n          <router-link to=\"/chat\" class=\"nav-item\">\n            <span class=\"cloud-icon\">💭</span>\n            <span>聊天</span>\n          </router-link>\n        </div>\n      </div>\n    </nav>\n\n    <div class=\"blog-content\">\n      <!-- 左侧博主介绍 -->\n      <div class=\"blog-sidebar-left\">\n        <div class=\"author-card sweet-card\">\n          <div class=\"author-avatar\">\n            <div class=\"avatar-placeholder\">\n              <span class=\"avatar-emoji\">🌸</span>\n            </div>\n          </div>\n          <h3 class=\"author-name gradient-text\">甜梦星球</h3>\n          <p class=\"author-bio\">在这里分享生活中的美好瞬间 ✨</p>\n          <div class=\"author-stats\">\n            <div class=\"stat-item\">\n              <span class=\"stat-value\">{{ postsCount }}</span>\n              <span class=\"stat-label\">文章</span>\n            </div>\n            <div class=\"stat-item\">\n              <span class=\"stat-value\">{{ categoriesCount }}</span>\n              <span class=\"stat-label\">分类</span>\n            </div>\n          </div>\n        </div>\n      </div>\n\n      <!-- 文章列表区域 -->\n      <div class=\"blog-main\">\n        <div v-if=\"loading\" class=\"loading-container\">\n          <div class=\"loading-animation\"></div>\n          <p>正在加载文章...</p>\n        </div>\n        \n        <div v-else-if=\"posts.length\" class=\"posts-grid\">\n          <div v-for=\"post in posts\" \n               :key=\"post._id\" \n               class=\"post-card sweet-card\"\n               @click=\"goToPost(post._id)\"\n               @mouseenter=\"animateEmoji(post._id)\">\n            <div class=\"post-image-container\">\n              <div class=\"post-image\" :style=\"{ backgroundImage: `url(${post.coverImage || './placeholder.jpg'})` }\"></div>\n            </div>\n            <div class=\"post-content\">\n              <h2 class=\"post-title\">\n                {{ post.title }}\n                <span class=\"post-emoji\" :class=\"{ 'animate': post.isAnimating }\">{{ getCategoryEmoji(post.category) }}</span>\n              </h2>\n              <div class=\"post-meta\">\n                <span class=\"post-date\">{{ formatDate(post.createdAt) }}</span>\n                <span class=\"post-category\">{{ post.category }}</span>\n              </div>\n              <p class=\"post-excerpt\">{{ post.excerpt }}</p>\n              <div class=\"post-footer\">\n                <div class=\"post-tags\">\n                  <span v-for=\"(tag, index) in post.tags\" \n                        :key=\"index\" \n                        class=\"post-tag\"\n                        :style=\"{ backgroundColor: getTagColor(tag) }\">\n                    {{ tag }}\n                  </span>\n                </div>\n                <button class=\"read-more sweet-btn\">阅读全文</button>\n              </div>\n            </div>\n          </div>\n        </div>\n        \n        <div v-else class=\"empty-posts sweet-card\">\n          <div class=\"empty-icon\">📝</div>\n          <p>暂无文章</p>\n          <p class=\"empty-suggestion\">成为第一个发布文章的人吧！</p>\n          <router-link to=\"/posts/create\" class=\"sweet-btn create-post-btn\">创建文章</router-link>\n        </div>\n      </div>\n\n      <!-- 右侧边栏 -->\n      <div class=\"blog-sidebar-right\">\n        <div class=\"sidebar-section sweet-card\">\n          <h3 class=\"sidebar-title gradient-text\">分类</h3>\n          <ul class=\"categories-list\">\n            <li class=\"category-item\" \n                :class=\"{ active: activeCategory === null }\" \n                @click=\"filterByCategory(null)\">\n              <span class=\"category-icon\">📚</span>\n              <span class=\"category-name\">全部</span>\n              <span class=\"category-count\">{{ posts.length }}</span>\n            </li>\n            <li v-for=\"category in categories\" \n                :key=\"category._id\" \n                class=\"category-item\"\n                :class=\"{ active: activeCategory === category._id }\"\n                @click=\"filterByCategory(category._id)\">\n              <span class=\"category-icon\">{{ category.icon }}</span>\n              <span class=\"category-name\">{{ category.name }}</span>\n              <span class=\"category-count\">{{ getCategoryCount(category._id) }}</span>\n            </li>\n          </ul>\n        </div>\n        \n        <div class=\"sidebar-section sweet-card\">\n          <h3 class=\"sidebar-title gradient-text\">标签云</h3>\n          <div class=\"tags-cloud\">\n            <span v-for=\"(tag, index) in popularTags\" \n                  :key=\"index\" \n                  class=\"tag-item\"\n                  :class=\"{ active: activeTag === tag }\" \n                  :style=\"{ backgroundColor: getTagColor(tag) }\"\n                  @click=\"filterByTag(tag)\">\n              {{ tag }}\n            </span>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'BlogView',\n  data() {\n    return {\n      posts: [],\n      loading: true,\n      activeCategory: null,\n      activeTag: null,\n      categories: [\n        { _id: '1', name: '旅行日记', icon: '🧳' },\n        { _id: '2', name: '美食探索', icon: '🍰' },\n        { _id: '3', name: '手工制作', icon: '🧶' },\n        { _id: '4', name: '生活随笔', icon: '✏️' }\n      ],\n      popularTags: ['甜点', '旅行', '手工', '阅读', '音乐', '电影', '宠物', '心情'],\n      tagColors: {\n        '甜点': '#FFB6C1',\n        '旅行': '#87CEEB',\n        '手工': '#DDA0DD',\n        '阅读': '#98FB98',\n        '音乐': '#FFA07A',\n        '电影': '#87CEFA',\n        '宠物': '#F0E68C',\n        '心情': '#E6E6FA'\n      }\n    };\n  },\n  computed: {\n    postsCount() {\n      return this.posts.length;\n    },\n    categoriesCount() {\n      return this.categories.length;\n    }\n  },\n  mounted() {\n    // 模拟获取文章数据\n    setTimeout(() => {\n      this.posts = this.getDummyPosts();\n      this.loading = false;\n    }, 1000);\n  },\n  methods: {\n    goToPost(id) {\n      this.$router.push(`/posts/${id}`);\n    },\n    formatDate(dateString) {\n      const date = new Date(dateString);\n      return date.toLocaleDateString('zh-CN', {\n        year: 'numeric',\n        month: 'long',\n        day: 'numeric'\n      });\n    },\n    filterByCategory(categoryId) {\n      this.activeCategory = categoryId;\n      // 在实际应用中，这里会重新获取筛选后的文章\n    },\n    filterByTag(tag) {\n      this.activeTag = this.activeTag === tag ? null : tag;\n      // 在实际应用中，这里会重新获取筛选后的文章\n    },\n    getDummyPosts() {\n      // 模拟文章数据\n      return [\n        {\n          _id: '1',\n          title: '樱花季的京都之旅',\n          excerpt: '春季的京都被樱花装点得如梦似幻，漫步在古老的街道上，感受千年文化与自然的完美融合...',\n          coverImage: 'https://via.placeholder.com/600x400?text=樱花季的京都之旅',\n          category: '旅行日记',\n          tags: ['旅行', '日本', '樱花'],\n          createdAt: '2023-04-12T08:00:00.000Z'\n        },\n        {\n          _id: '2',\n          title: '自制云朵棉花糖食谱',\n          excerpt: '用简单的材料在家制作蓬松柔软的云朵棉花糖，为下午茶时光增添一抹甜蜜...',\n          coverImage: 'https://via.placeholder.com/600x400?text=自制云朵棉花糖食谱',\n          category: '美食探索',\n          tags: ['甜点', '食谱', '手工'],\n          createdAt: '2023-05-06T10:30:00.000Z'\n        },\n        {\n          _id: '3',\n          title: '星空主题钩针小物',\n          excerpt: '分享一款简单的星星图案钩针小包教程，适合初学者尝试的可爱手工项目...',\n          coverImage: 'https://via.placeholder.com/600x400?text=星空主题钩针小物',\n          category: '手工制作',\n          tags: ['手工', '钩针', '教程'],\n          createdAt: '2023-06-18T14:15:00.000Z'\n        }\n      ];\n    },\n    getCategoryEmoji(category) {\n      const emojiMap = {\n        '旅行日记': '🧳',\n        '美食探索': '🍰',\n        '手工制作': '🧶',\n        '生活随笔': '✏️'\n      };\n      return emojiMap[category] || '📝';\n    },\n    animateEmoji(postId) {\n      const post = this.posts.find(p => p._id === postId);\n      if (post) {\n        post.isAnimating = true;\n        setTimeout(() => {\n          post.isAnimating = false;\n        }, 1000);\n      }\n    },\n    getTagColor(tag) {\n      return this.tagColors[tag] || '#87CEEB';\n    },\n    getCategoryCount(categoryId) {\n      return this.posts.filter(post => {\n        const category = this.categories.find(c => c._id === categoryId);\n        return category && post.category === category.name;\n      }).length;\n    }\n  }\n};\n</script>\n\n<style scoped>\n/* 云朵导航栏样式 */\n.cloud-nav {\n  background: linear-gradient(to right, #FFB6C1, #87CEEB);\n  padding: 15px 0;\n  margin-bottom: 30px;\n  border-radius: 30px;\n}\n\n.cloud-nav-content {\n  max-width: 800px;\n  margin: 0 auto;\n}\n\n.nav-cloud {\n  display: flex;\n  justify-content: space-around;\n  align-items: center;\n  background: rgba(255, 255, 255, 0.9);\n  border-radius: 25px;\n  padding: 10px;\n}\n\n.nav-item {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  text-decoration: none;\n  color: var(--text-primary);\n  padding: 10px 20px;\n  border-radius: 20px;\n  transition: all 0.3s ease;\n}\n\n.nav-item:hover, .nav-item.active {\n  background: rgba(255, 255, 255, 0.8);\n  transform: translateY(-2px);\n}\n\n.cloud-icon {\n  font-size: 1.5rem;\n  margin-bottom: 5px;\n}\n\n/* 博主介绍卡片样式 */\n.author-card {\n  padding: 20px;\n  text-align: center;\n  margin-bottom: 30px;\n}\n\n.author-avatar {\n  width: 120px;\n  height: 120px;\n  margin: 0 auto 15px;\n  border-radius: 60px;\n  overflow: hidden;\n  border: 4px solid #FFB6C1;\n  background: linear-gradient(45deg, #FFB6C1, #87CEEB);\n}\n\n.avatar-placeholder {\n  width: 100%;\n  height: 100%;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  animation: float 3s ease-in-out infinite;\n}\n\n.avatar-emoji {\n  font-size: 3rem;\n  animation: rotate 6s linear infinite;\n}\n\n@keyframes rotate {\n  from { transform: rotate(0deg); }\n  to { transform: rotate(360deg); }\n}\n\n.author-name {\n  font-size: 1.5rem;\n  margin-bottom: 10px;\n}\n\n.author-bio {\n  color: var(--text-secondary);\n  margin-bottom: 15px;\n}\n\n.author-stats {\n  display: flex;\n  justify-content: center;\n  gap: 20px;\n}\n\n.stat-item {\n  text-align: center;\n}\n\n.stat-value {\n  font-size: 1.2rem;\n  font-weight: bold;\n  color: var(--primary-purple);\n}\n\n.stat-label {\n  font-size: 0.9rem;\n  color: var(--text-secondary);\n}\n\n/* 文章卡片样式 */\n.post-card {\n  border-radius: 15px;\n  overflow: hidden;\n  transition: all 0.3s ease;\n}\n\n.post-card:hover {\n  transform: translateY(-5px);\n  box-shadow: 0 10px 20px rgba(0, 0, 0, 0.1);\n}\n\n.post-emoji {\n  display: inline-block;\n  margin-left: 10px;\n  transition: transform 0.3s ease;\n}\n\n.post-emoji.animate {\n  animation: bounce 1s ease;\n}\n\n/* 动画效果 */\n@keyframes float {\n  0%, 100% { transform: translateY(0); }\n  50% { transform: translateY(-10px); }\n}\n\n@keyframes bounce {\n  0%, 100% { transform: scale(1); }\n  50% { transform: scale(1.3); }\n}\n\n/* 标签样式 */\n.post-tag {\n  padding: 4px 12px;\n  border-radius: 15px;\n  font-size: 0.9rem;\n  color: white;\n  margin-right: 8px;\n  transition: all 0.3s ease;\n}\n\n.post-tag:hover {\n  transform: translateY(-2px);\n  box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);\n}\n\n/* 响应式布局 */\n@media (max-width: 1200px) {\n  .blog-content {\n    grid-template-columns: 1fr;\n  }\n  \n  .blog-sidebar-left, .blog-sidebar-right {\n    display: none;\n  }\n}\n\n/* 使用可爱手写体 */\n@font-face {\n  font-family: 'Cute Font';\n  src: url('https://fonts.googleapis.com/css2?family=Cute+Font&display=swap');\n}\n\n.blog {\n  font-family: 'Cute Font', sans-serif;\n}\n</style>"],"mappings":";;;;AA8IA,eAAe;EACbA,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE,IAAI;MACbC,cAAc,EAAE,IAAI;MACpBC,SAAS,EAAE,IAAI;MACfC,UAAU,EAAE,CACV;QAAEC,GAAG,EAAE,GAAG;QAAEP,IAAI,EAAE,MAAM;QAAEQ,IAAI,EAAE;MAAK,CAAC,EACtC;QAAED,GAAG,EAAE,GAAG;QAAEP,IAAI,EAAE,MAAM;QAAEQ,IAAI,EAAE;MAAK,CAAC,EACtC;QAAED,GAAG,EAAE,GAAG;QAAEP,IAAI,EAAE,MAAM;QAAEQ,IAAI,EAAE;MAAK,CAAC,EACtC;QAAED,GAAG,EAAE,GAAG;QAAEP,IAAI,EAAE,MAAM;QAAEQ,IAAI,EAAE;MAAK,EACtC;MACDC,WAAW,EAAE,CAAC,IAAI,EAAE,IAAI,EAAE,IAAI,EAAE,IAAI,EAAE,IAAI,EAAE,IAAI,EAAE,IAAI,EAAE,IAAI,CAAC;MAC7DC,SAAS,EAAE;QACT,IAAI,EAAE,SAAS;QACf,IAAI,EAAE,SAAS;QACf,IAAI,EAAE,SAAS;QACf,IAAI,EAAE,SAAS;QACf,IAAI,EAAE,SAAS;QACf,IAAI,EAAE,SAAS;QACf,IAAI,EAAE,SAAS;QACf,IAAI,EAAE;MACR;IACF,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,UAAUA,CAAA,EAAG;MACX,OAAO,IAAI,CAACV,KAAK,CAACW,MAAM;IAC1B,CAAC;IACDC,eAAeA,CAAA,EAAG;MAChB,OAAO,IAAI,CAACR,UAAU,CAACO,MAAM;IAC/B;EACF,CAAC;EACDE,OAAOA,CAAA,EAAG;IACR;IACAC,UAAU,CAAC,MAAM;MACf,IAAI,CAACd,KAAI,GAAI,IAAI,CAACe,aAAa,CAAC,CAAC;MACjC,IAAI,CAACd,OAAM,GAAI,KAAK;IACtB,CAAC,EAAE,IAAI,CAAC;EACV,CAAC;EACDe,OAAO,EAAE;IACPC,QAAQA,CAACC,EAAE,EAAE;MACX,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,UAAUF,EAAE,EAAE,CAAC;IACnC,CAAC;IACDG,UAAUA,CAACC,UAAU,EAAE;MACrB,MAAMC,IAAG,GAAI,IAAIC,IAAI,CAACF,UAAU,CAAC;MACjC,OAAOC,IAAI,CAACE,kBAAkB,CAAC,OAAO,EAAE;QACtCC,IAAI,EAAE,SAAS;QACfC,KAAK,EAAE,MAAM;QACbC,GAAG,EAAE;MACP,CAAC,CAAC;IACJ,CAAC;IACDC,gBAAgBA,CAACC,UAAU,EAAE;MAC3B,IAAI,CAAC5B,cAAa,GAAI4B,UAAU;MAChC;IACF,CAAC;IACDC,WAAWA,C