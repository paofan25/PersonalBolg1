{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.find.js\";\nexport default {\n  name: 'BlogView',\n  data() {\n    return {\n      posts: [],\n      loading: true,\n      activeCategory: null,\n      activeTag: null,\n      categories: [{\n        _id: '1',\n        name: 'ÊóÖË°åÊó•ËÆ∞',\n        icon: 'üß≥'\n      }, {\n        _id: '2',\n        name: 'ÁæéÈ£üÊé¢Á¥¢',\n        icon: 'üç∞'\n      }, {\n        _id: '3',\n        name: 'ÊâãÂ∑•Âà∂‰Ωú',\n        icon: 'üß∂'\n      }, {\n        _id: '4',\n        name: 'ÁîüÊ¥ªÈöèÁ¨î',\n        icon: '‚úèÔ∏è'\n      }],\n      popularTags: ['ÁîúÁÇπ', 'ÊóÖË°å', 'ÊâãÂ∑•', 'ÈòÖËØª', 'Èü≥‰πê', 'ÁîµÂΩ±', 'ÂÆ†Áâ©', 'ÂøÉÊÉÖ'],\n      tagColors: {\n        'ÁîúÁÇπ': '#FFB6C1',\n        'ÊóÖË°å': '#87CEEB',\n        'ÊâãÂ∑•': '#DDA0DD',\n        'ÈòÖËØª': '#98FB98',\n        'Èü≥‰πê': '#FFA07A',\n        'ÁîµÂΩ±': '#87CEFA',\n        'ÂÆ†Áâ©': '#F0E68C',\n        'ÂøÉÊÉÖ': '#E6E6FA'\n      }\n    };\n  },\n  computed: {\n    postsCount() {\n      return this.posts.length;\n    },\n    categoriesCount() {\n      return this.categories.length;\n    }\n  },\n  mounted() {\n    // Ê®°ÊãüËé∑ÂèñÊñáÁ´†Êï∞ÊçÆ\n    setTimeout(() => {\n      this.posts = this.getDummyPosts();\n      this.loading = false;\n    }, 1000);\n  },\n  methods: {\n    goToPost(id) {\n      this.$router.push(`/posts/${id}`);\n    },\n    formatDate(dateString) {\n      const date = new Date(dateString);\n      return date.toLocaleDateString('zh-CN', {\n        year: 'numeric',\n        month: 'long',\n        day: 'numeric'\n      });\n    },\n    filterByCategory(categoryId) {\n      this.activeCategory = categoryId;\n      // Âú®ÂÆûÈôÖÂ∫îÁî®‰∏≠ÔºåËøôÈáå‰ºöÈáçÊñ∞Ëé∑ÂèñÁ≠õÈÄâÂêéÁöÑÊñáÁ´†\n    },\n    filterByTag(tag) {\n      this.activeTag = this.activeTag === tag ? null : tag;\n      // Âú®ÂÆûÈôÖÂ∫îÁî®‰∏≠ÔºåËøôÈáå‰ºöÈáçÊñ∞Ëé∑ÂèñÁ≠õÈÄâÂêéÁöÑÊñáÁ´†\n    },\n    getDummyPosts() {\n      // Ê®°ÊãüÊñáÁ´†Êï∞ÊçÆ\n      return [{\n        _id: '1',\n        title: 'Ê®±Ëä±Â≠£ÁöÑ‰∫¨ÈÉΩ‰πãÊóÖ',\n        excerpt: 'Êò•Â≠£ÁöÑ‰∫¨ÈÉΩË¢´Ê®±Ëä±Ë£ÖÁÇπÂæóÂ¶ÇÊ¢¶‰ººÂπªÔºåÊº´Ê≠•Âú®Âè§ËÄÅÁöÑË°óÈÅì‰∏äÔºåÊÑüÂèóÂçÉÂπ¥ÊñáÂåñ‰∏éËá™ÁÑ∂ÁöÑÂÆåÁæéËûçÂêà...',\n        coverImage: 'https://via.placeholder.com/600x400?text=Ê®±Ëä±Â≠£ÁöÑ‰∫¨ÈÉΩ‰πãÊóÖ',\n        category: 'ÊóÖË°åÊó•ËÆ∞',\n        tags: ['ÊóÖË°å', 'Êó•Êú¨', 'Ê®±Ëä±'],\n        createdAt: '2023-04-12T08:00:00.000Z'\n      }, {\n        _id: '2',\n        title: 'Ëá™Âà∂‰∫ëÊúµÊ£âËä±Á≥ñÈ£üË∞±',\n        excerpt: 'Áî®ÁÆÄÂçïÁöÑÊùêÊñôÂú®ÂÆ∂Âà∂‰ΩúËì¨ÊùæÊüîËΩØÁöÑ‰∫ëÊúµÊ£âËä±Á≥ñÔºå‰∏∫‰∏ãÂçàËå∂Êó∂ÂÖâÂ¢ûÊ∑ª‰∏ÄÊäπÁîúËúú...',\n        coverImage: 'https://via.placeholder.com/600x400?text=Ëá™Âà∂‰∫ëÊúµÊ£âËä±Á≥ñÈ£üË∞±',\n        category: 'ÁæéÈ£üÊé¢Á¥¢',\n        tags: ['ÁîúÁÇπ', 'È£üË∞±', 'ÊâãÂ∑•'],\n        createdAt: '2023-05-06T10:30:00.000Z'\n      }, {\n        _id: '3',\n        title: 'ÊòüÁ©∫‰∏ªÈ¢òÈí©ÈíàÂ∞èÁâ©',\n        excerpt: 'ÂàÜ‰∫´‰∏ÄÊ¨æÁÆÄÂçïÁöÑÊòüÊòüÂõæÊ°àÈí©ÈíàÂ∞èÂåÖÊïôÁ®ãÔºåÈÄÇÂêàÂàùÂ≠¶ËÄÖÂ∞ùËØïÁöÑÂèØÁà±ÊâãÂ∑•È°πÁõÆ...',\n        coverImage: 'https://via.placeholder.com/600x400?text=ÊòüÁ©∫‰∏ªÈ¢òÈí©ÈíàÂ∞èÁâ©',\n        category: 'ÊâãÂ∑•Âà∂‰Ωú',\n        tags: ['ÊâãÂ∑•', 'Èí©Èíà', 'ÊïôÁ®ã'],\n        createdAt: '2023-06-18T14:15:00.000Z'\n      }];\n    },\n    getCategoryEmoji(category) {\n      const emojiMap = {\n        'ÊóÖË°åÊó•ËÆ∞': 'üß≥',\n        'ÁæéÈ£üÊé¢Á¥¢': 'üç∞',\n        'ÊâãÂ∑•Âà∂‰Ωú': 'üß∂',\n        'ÁîüÊ¥ªÈöèÁ¨î': '‚úèÔ∏è'\n      };\n      return emojiMap[category] || 'üìù';\n    },\n    animateEmoji(postId) {\n      const post = this.posts.find(p => p._id === postId);\n      if (post) {\n        post.isAnimating = true;\n        setTimeout(() => {\n          post.isAnimating = false;\n        }, 1000);\n      }\n    },\n    getTagColor(tag) {\n      return this.tagColors[tag] || '#87CEEB';\n    },\n    getCategoryCount(categoryId) {\n      return this.posts.filter(post => {\n        const category = this.categories.find(c => c._id === categoryId);\n        return category && post.category === category.name;\n      }).length;\n    }\n  }\n};","map":{"version":3,"names":["name","data","posts","loading","activeCategory","activeTag","categories","_id","icon","popularTags","tagColors","computed","postsCount","length","categoriesCount","mounted","setTimeout","getDummyPosts","methods","goToPost","id","$router","push","formatDate","dateString","date","Date","toLocaleDateString","year","month","day","filterByCategory","categoryId","filterByTag","tag","title","excerpt","coverImage","category","tags","createdAt","getCategoryEmoji","emojiMap","animateEmoji","postId","post","find","p","isAnimating","getTagColor","getCategoryCount","filter","c"],"sources":["E:\\code\\PersonalBolg1\\frontend\\src\\views\\BlogView.vue"],"sourcesContent":["<template>\n  <div class=\"blog\">\n    <!-- ‰∫ëÊúµÂØºËà™Ê†è -->\n    <nav class=\"cloud-nav sweet-card\">\n      <div class=\"cloud-nav-content\">\n        <div class=\"nav-cloud\">\n          <router-link to=\"/\" class=\"nav-item\">\n            <span class=\"cloud-icon\">üè†</span>\n            <span>È¶ñÈ°µ</span>\n          </router-link>\n          <router-link to=\"/blog\" class=\"nav-item active\">\n            <span class=\"cloud-icon\">üìö</span>\n            <span>ÂçöÂÆ¢</span>\n          </router-link>\n          <router-link to=\"/games\" class=\"nav-item\">\n            <span class=\"cloud-icon\">üéÆ</span>\n            <span>Ê∏∏Êàè</span>\n          </router-link>\n          <router-link to=\"/chat\" class=\"nav-item\">\n            <span class=\"cloud-icon\">üí≠</span>\n            <span>ËÅäÂ§©</span>\n          </router-link>\n        </div>\n      </div>\n    </nav>\n\n    <div class=\"blog-content\">\n      <!-- Â∑¶‰æßÂçö‰∏ª‰ªãÁªç -->\n      <div class=\"blog-sidebar-left\">\n        <div class=\"author-card sweet-card\">\n          <div class=\"author-avatar\">\n            <div class=\"avatar-placeholder\">\n              <span class=\"avatar-emoji\">üå∏</span>\n            </div>\n          </div>\n          <h3 class=\"author-name gradient-text\">ÁîúÊ¢¶ÊòüÁêÉ</h3>\n          <p class=\"author-bio\">Âú®ËøôÈáåÂàÜ‰∫´ÁîüÊ¥ª‰∏≠ÁöÑÁæéÂ•ΩÁû¨Èó¥ ‚ú®</p>\n          <div class=\"author-stats\">\n            <div class=\"stat-item\">\n              <span class=\"stat-value\">{{ postsCount }}</span>\n              <span class=\"stat-label\">ÊñáÁ´†</span>\n            </div>\n            <div class=\"stat-item\">\n              <span class=\"stat-value\">{{ categoriesCount }}</span>\n              <span class=\"stat-label\">ÂàÜÁ±ª</span>\n            </div>\n          </div>\n        </div>\n      </div>\n\n      <!-- ÊñáÁ´†ÂàóË°®Âå∫Âüü -->\n      <div class=\"blog-main\">\n        <div v-if=\"loading\" class=\"loading-container\">\n          <div class=\"loading-animation\"></div>\n          <p>Ê≠£Âú®Âä†ËΩΩÊñáÁ´†...</p>\n        </div>\n        \n        <div v-else-if=\"posts.length\" class=\"posts-grid\">\n          <div v-for=\"post in posts\" \n               :key=\"post._id\" \n               class=\"post-card sweet-card\"\n               @click=\"goToPost(post._id)\"\n               @mouseenter=\"animateEmoji(post._id)\">\n            <div class=\"post-image-container\">\n              <div class=\"post-image\" :style=\"{ backgroundImage: `url(${post.coverImage || './placeholder.jpg'})` }\"></div>\n            </div>\n            <div class=\"post-content\">\n              <h2 class=\"post-title\">\n                {{ post.title }}\n                <span class=\"post-emoji\" :class=\"{ 'animate': post.isAnimating }\">{{ getCategoryEmoji(post.category) }}</span>\n              </h2>\n              <div class=\"post-meta\">\n                <span class=\"post-date\">{{ formatDate(post.createdAt) }}</span>\n                <span class=\"post-category\">{{ post.category }}</span>\n              </div>\n              <p class=\"post-excerpt\">{{ post.excerpt }}</p>\n              <div class=\"post-footer\">\n                <div class=\"post-tags\">\n                  <span v-for=\"(tag, index) in post.tags\" \n                        :key=\"index\" \n                        class=\"post-tag\"\n                        :style=\"{ backgroundColor: getTagColor(tag) }\">\n                    {{ tag }}\n                  </span>\n                </div>\n                <button class=\"read-more sweet-btn\">ÈòÖËØªÂÖ®Êñá</button>\n              </div>\n            </div>\n          </div>\n        </div>\n        \n        <div v-else class=\"empty-posts sweet-card\">\n          <div class=\"empty-icon\">üìù</div>\n          <p>ÊöÇÊó†ÊñáÁ´†</p>\n          <p class=\"empty-suggestion\">Êàê‰∏∫Á¨¨‰∏Ä‰∏™ÂèëÂ∏ÉÊñáÁ´†ÁöÑ‰∫∫ÂêßÔºÅ</p>\n          <router-link to=\"/posts/create\" class=\"sweet-btn create-post-btn\">ÂàõÂª∫ÊñáÁ´†</router-link>\n        </div>\n      </div>\n\n      <!-- Âè≥‰æßËæπÊ†è -->\n      <div class=\"blog-sidebar-right\">\n        <div class=\"sidebar-section sweet-card\">\n          <h3 class=\"sidebar-title gradient-text\">ÂàÜÁ±ª</h3>\n          <ul class=\"categories-list\">\n            <li class=\"category-item\" \n                :class=\"{ active: activeCategory === null }\" \n                @click=\"filterByCategory(null)\">\n              <span class=\"category-icon\">üìö</span>\n              <span class=\"category-name\">ÂÖ®ÈÉ®</span>\n              <span class=\"category-count\">{{ posts.length }}</span>\n            </li>\n            <li v-for=\"category in categories\" \n                :key=\"category._id\" \n                class=\"category-item\"\n                :class=\"{ active: activeCategory === category._id }\"\n                @click=\"filterByCategory(category._id)\">\n              <span class=\"category-icon\">{{ category.icon }}</span>\n              <span class=\"category-name\">{{ category.name }}</span>\n              <span class=\"category-count\">{{ getCategoryCount(category._id) }}</span>\n            </li>\n          </ul>\n        </div>\n        \n        <div class=\"sidebar-section sweet-card\">\n          <h3 class=\"sidebar-title gradient-text\">Ê†áÁ≠æ‰∫ë</h3>\n          <div class=\"tags-cloud\">\n            <span v-for=\"(tag, index) in popularTags\" \n                  :key=\"index\" \n                  class=\"tag-item\"\n                  :class=\"{ active: activeTag === tag }\" \n                  :style=\"{ backgroundColor: getTagColor(tag) }\"\n                  @click=\"filterByTag(tag)\">\n              {{ tag }}\n            </span>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'BlogView',\n  data() {\n    return {\n      posts: [],\n      loading: true,\n      activeCategory: null,\n      activeTag: null,\n      categories: [\n        { _id: '1', name: 'ÊóÖË°åÊó•ËÆ∞', icon: 'üß≥' },\n        { _id: '2', name: 'ÁæéÈ£üÊé¢Á¥¢', icon: 'üç∞' },\n        { _id: '3', name: 'ÊâãÂ∑•Âà∂‰Ωú', icon: 'üß∂' },\n        { _id: '4', name: 'ÁîüÊ¥ªÈöèÁ¨î', icon: '‚úèÔ∏è' }\n      ],\n      popularTags: ['ÁîúÁÇπ', 'ÊóÖË°å', 'ÊâãÂ∑•', 'ÈòÖËØª', 'Èü≥‰πê', 'ÁîµÂΩ±', 'ÂÆ†Áâ©', 'ÂøÉÊÉÖ'],\n      tagColors: {\n        'ÁîúÁÇπ': '#FFB6C1',\n        'ÊóÖË°å': '#87CEEB',\n        'ÊâãÂ∑•': '#DDA0DD',\n        'ÈòÖËØª': '#98FB98',\n        'Èü≥‰πê': '#FFA07A',\n        'ÁîµÂΩ±': '#87CEFA',\n        'ÂÆ†Áâ©': '#F0E68C',\n        'ÂøÉÊÉÖ': '#E6E6FA'\n      }\n    };\n  },\n  computed: {\n    postsCount() {\n      return this.posts.length;\n    },\n    categoriesCount() {\n      return this.categories.length;\n    }\n  },\n  mounted() {\n    // Ê®°ÊãüËé∑ÂèñÊñáÁ´†Êï∞ÊçÆ\n    setTimeout(() => {\n      this.posts = this.getDummyPosts();\n      this.loading = false;\n    }, 1000);\n  },\n  methods: {\n    goToPost(id) {\n      this.$router.push(`/posts/${id}`);\n    },\n    formatDate(dateString) {\n      const da